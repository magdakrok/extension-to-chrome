{
    "name": "Cookbook",
    "version": "1.0",
    "description": "Extension to cookbook app",
    "permissions": ["activeTab","declarativeContent","storage"],
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
      "default_popup": "popup.html",
    
    "default_icon": {
      "16": "images/cake.png",
      "32": "images/cake32px.png",
      "64": "images/cake64px.png",
      "128": "images/cake128px.png"
    }
  },
 
    "manifest_version": 2
  }